  display: flex;
  align-items: center;
  flex-direction: row;
  margin-bottom: 10rpx;
}
.index_top .page_top .idiom_head .grade .name {
  margin-left: 10rpx;
  font-size: 36rpx;
  justify-content: center;
}
.index_top .page_top .topic {
  width: 300rpx;
  height: 75rpx;
  line-height: 46rpx;
  font-size: 28rpx;
  text-align: center;
  position: absolute;
  left: 250rpx;
  margin-left: -92rpx;
  top: -16rpx;
  color: #5b1404;
  font-size: 30rpx;
