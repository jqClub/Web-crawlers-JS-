/*答题区*/
.idiom_inputbox {
  position: relative;
  width: 380rpx;
  margin: 30rpx auto 0;
  display: flex;
  justify-content: space-between;
  align-items: center;
}
.idiom_inpu {
  position: relative;
  display: flex;
  justify-content: center;
  align-items: center;
  width: 80rpx;
  height: 80rpx;
  z-index: 4;
  font-size: 40rpx;
  border-radius: 10rpx;
  color: white;
  background: #569dff;
  box-shadow: 0 8rpx 0 0 #437dcd;
}
.idiom_inpu.wobble {
  color: red;
}
.reset {
  width: 48rpx;
  height: 48rpx;
}

/*答题按钮*/
.idiom_buttonbox {
  display: flex;
  flex-wrap: wrap;
  align-items: center;
  justify-content: center;
  margin: 0 12rpx;
}
.idiom_buttonbox .idiom_button {
  box-sizing: border-box;
  position: relative;
  margin: 10rpx;
  width: 80rpx;
  height: 80rpx;
  line-height: 76rpx;
  border-radius: 10rpx;
  text-align: center;
  font-size: 40rpx;
  color: #222c94;
  background: #ffffff;
  transition: all 0.3s;
}

/*按钮消失动画*/
.hide_button {
  z-index: 0;
  transform: translateY(-200rpx) scale(0);
  opacity: 0;
  pointer-events: none;
}

/*底部提示*/
.idiom_msg_box {
  position: fixed;
  bottom: 40%;
  left: 0;
  right: 0;
  z-index: 10;
  display: flex;
  justify-content: center;
  align-items: center;
  width: 100%;
  line-height: 80rpx;
  font-size: 28rpx;
  transition: all 0.3s;
  transform: translateY(200rpx);
  opacity: 0;
  pointer-events: none;
}
.idiom_msg_box .idiom_msg {
  text-align: center;
  display: flex;
  justify-content: center;
  align-items: center;
  padding: 0rpx 28rpx;
  box-shadow: 4rpx 6rpx 0 rgba(0, 0, 0, 0.3);
}
.idiom_msg_box .Correct_box {
  background: #ff9921;
  color: #ffffff;
  border-radius: 20rem;
  border: #f7d967 solid 4rpx;
}
.idiom_msg_box .Error_box {
  background: #575757;
  color: #fff;
  border-radius: 20rem;
  border: #2e2e2e solid 4rpx;
}
.idiom_msg_box image {
  margin-right: 12rpx;
}

/*底部导航*/
.guide_banner .banner_block {
  box-shadow: 6rpx 6rpx 0 #055165;
  width: 600rpx;
  text-align: center;
  margin: 30rpx auto 0rpx auto;
  border-radius: 8rpx;
  font-size: 28rpx;
  line-height: 70rpx;
}
.index_avatar {
  position: relative;
  width: 60rpx;
  height: 60rpx;
  border-radius: 50%;
  flex: none;
}

/* 题目 */
.question {
  position: relative;
  margin-top: 60rpx;
  padding-bottom: 24rpx;
  width: 680rpx;
  text-align: center;
  border-radius: 20rpx;
  background: #000b48;
}
.question__header {
  position: absolute;
  top: -44rpx;
  left: 0;
  right: 0;
  margin: auto;
}
.question__header_img {
  width: 400rpx;
  height: 118rpx;
}
.question__header_text {
  position: absolute;
  top: 20rpx;
  left: 0;
  right: 0;
  margin: auto;
  font-size: 36rpx;
  color: #fff;
}
.question__pic {
  position: relative;
  display: inline-block;
  margin-top: 100rpx;
  width: 376rpx;
  height: 286rpx;
  border-radius: 20rpx;
  background: #efe5cc;
}
.question__loading {
  position: absolute;
  top: 0;
  bottom: 0;
  left: 0;
  right: 0;
  margin: auto;
  width: 64rpx;
  height: 64rpx;
}
.question__btns {
  margin-top: 30rpx;
  display: flex;
  justify-content: center;
}
.question__btn {
  margin: 0 10rpx;
  width: 180rpx;
  line-height: 72rpx;
  border-radius: 72rpx;
  font-size: 36rpx;
  color: #ffffff;
}
.question__tip {
  background: #44c35a;
  box-shadow: 0 8rpx 0 0 #30883f;
}
.question__help {
  background: #ef4c4c;
  box-shadow: 0 8rpx 0 0 #8d2b2b;
}
.question__share {
  background: #ff712e;
  box-shadow: 0 8rpx 0 0 #9c4b25;
}

/* 金币 */
.gold {
  display: flex;
  justify-content: center;
  align-items: center;
  margin-top: 30rpx;
}
.gold__icon {
  margin-right: 12rpx;
  width: 40rpx;
  height: 40rpx;
  border-radius: 50%;
}
.gold__text {
  font-size: 36rpx;
  color: #ffffff;
}

/* 遮罩 */
.mask__fixe {
  position: fixed;
  top: 0;
  left: 0;
  z-index: 9;
  width: 100%;
  height: 100%;
  background: rgba(0, 0, 0, 0.8);
}
.mask__box {
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  box-sizing: border-box;
  padding: 80rpx 0;
  height: 100%;
}
.mask__bg {
  position: absolute;
  left: 0;
  width: 100vw;
}
.mask__bg2 {
  position: absolute;
  z-index: 9;
  top: 20rpx;
  left: 20rpx;
  width: 354rpx;
  height: 196rp;
}
.mask__wrap {
  position: relative;
  /* top: -60rpx; */
  border-radius: 10rpx;
  overflow: auto;
}
.mask__icon {
  position: absolute;
  top: 0;
  right: 0;
  padding: 40rpx;
  width: 40rpx;
  height: 40rpx;
}

/* 模态 */
.modal-header {
  position: relative;
  top: 46rpx;
  text-align: center;
}
.modal-header__img {
  width: 400rpx;
  height: 118rpx;
}
.modal-header__text {
  position: absolute;
  left: 0;
  right: 0;
  margin-top: 26rpx;
  font: 36rpx;
  color: #ffffff;
}

.modal {
  padding: 56rpx 0 44rpx;
  width: 600rpx;
  background: #222c94;
  border-radius: 40rpx;
}
.modal__title {
  font-size: 40rpx;
  color: #ffffff;
  text-align: center;
}
.modal__explain {
  box-sizing: border-box;
  margin: 20rpx 40rpx 0;
  padding: 24rpx 42rpx;
  min-height: 280rpx;
  border-radius: 12rpx;
  line-height: 1.5;
  font-size: 35rpx;
  word-break: break-all;
  color: #444444;
  background-color: #ffffff;
}
.modal__img {
  display: block;
  margin: 40rpx auto 0;
  width: 252rpx;
  height: 332rpx;
}
.modal__name {
  text-align: center;
  font-size: 48rpx;
  color: #ffdb47;
}
.modal__again_img {
  display: block;
  margin: auto;
  width: 400rpx;
  height: 400rpx;
}
.modal__again_text {
  text-align: center;
  font-size: 32rpx;
  color: #ffffff;
}
.modal__gold {
  margin-top: 46rpx;
  display: flex;
  justify-content: center;
  align-items: center;
}
.modal__gold_icon {
  margin-right: 12rpx;
  width: 40rpx;
  height: 40rpx;
}
.modal__gold_text {
  font-size: 36rpx;
  color: #ffffff;
}

.modal-btns {
  display: flex;
  justify-content: space-around;
  margin-top: 60rpx;
  margin-bottom: 20rpx;
}
.modal-btns__btn {
  width: 384rpx;
  line-height: 88rpx;
  border-radius: 88rpx;
  font-size: 36rpx;
  color: #ffffff;
  background: #569dff;
  box-shadow: 0 8rpx 0 0 #264b7e;
}
.modal-btns__btn--half {
  width: 272rpx;
}
.modal-btns__btn--explain {
  background: #44c35a;
}

/* 帮答人数 */
.help-total {
  padding: 30rpx 0;
  line-height: 1;
  font-size: 28rpx;
}
.help-total__text {
  color: #ffffff;
}
.help-total__num {
  color: #ffdb47;
}

/* 帮答列表 */
.help-list {
  width: 600rpx;
}
.help-list-header {
  line-height: 120rpx;
  text-align: center;
  font-size: 40rpx;
  color: #ffffff;
  background: #569dff;
}
.help-list-scroll {
  box-sizing: border-box;
  padding: 8rpx 40rpx;
  height: 680rpx;
  background: #ffffff;
}
.help-list-item {
  display: flex;
  align-items: center;
  height: 104rpx;
  border-bottom: 1px solid #eeeeee;
}
.help-list-item__head {
  margin-right: 20rpx;
  width: 80rpx;
  height: 80rpx;
  border-radius: 50%;
}
.help-list-item__name {
  flex: 1;
  overflow: hidden;
  white-space: nowrap;
  text-overflow: ellipsis;
  font-size: 32rpx;
  color: #444444;
}
.help-list-item__right {
  font-size: 36rpx;
  color: #44c35a;
}
.help-list-item__wrong {
  font-size: 36rpx;
  color: red;
}

/* 帮答返回 */
.help-to-me {
  margin: 80rpx auto;
  width: 480rpx;
  line-height: 88rpx;
  border-radius: 88rpx;
  font-size: 36rpx;
  color: #ffffff;
  background: #569dff;
  box-shadow: 0 8rpx 0 0 #264b7e;
}

/* 弹幕 */
.barrage {
  position: absolute;
  z-index: 999;
  top: 0;
  width: 100vw;
  overflow-x: hidden;
  transition: all 0.5s;
  pointer-events: none;
}
.barrage-item {
  position: absolute;
  display: flex;
  justify-content: center;
  align-items: center;
  padding: 6rpx;
  border-radius: 100rpx;
}
.barrage-item__img {
  width: 60rpx;
  height: 60rpx;
  border-radius: 50%;
}
.barrage-item__text {
  margin: 0 12rpx;
  word-break: break-all;
}
.barrage-item__icon {
  width: 40rpx;
  height: 40rpx;
}

@keyframes first {
  from {
    transform: translateX(750rpx);
  }
  to {
    transform: translateX(-750rpx);
  }
}

/* 答题组件 */
.my-banked-cloze {
  margin-top: 40rpx;
}

/* 广告 */
.ad {
  margin-top: 20rpx;
}
.ad-wrap {
  overflow: hidden;
  position: relative;
  animation: ad 1s 1s both;
}
@keyframes ad {
  0% {
    height: 0;
    min-height: 0;
  }
  90% {
    height: 0;
    min-height: 200rpx;
  }
  100% {
    height: auto;
    min-height: 200rpx;
  }
}

/* 拿答案 */
.get-answer {
  position: relative;
  margin: 26rpx 26rpx 0;
  width: 460rpx;
  border: solid 4rpx #ffffff;
  border-radius: 4rpx;
  text-align: center;
  color: #ffffff;
  background-color: #376bd1;
}
.get-answer__name {
  margin-top: 20rpx;
  width: 272rpx;
  height: 64rpx;
}
.get-answer__main {
  margin: 28rpx auto 40rpx;
  width: 400rpx;
  border-radius: 12rpx;
  background-color: #010a4b;
}
.get-answer__brand {
  margin-top: 40rpx;
  width: 120rpx;
  height: 120rpx;
}
.get-answer__title {
  margin-top: 26rpx;
  font-size: 48rpx;
}
.get-answer__text {
  margin-top: 58rpx;
  padding: 8rpx 20rpx;
  display: inline-block;
  border-radius: 8rpx;
  border: solid 1px #ffffff;
  font-size: 12px;
}
.get-answer__btn {
  display: inline-block;
  margin: 58rpx auto 44rpx;
  width: 214rpx;
  line-height: 68rpx;
  font-size: 36rpx;
  color: #376bd1;
  background-color: #ffffff;
}

.get-answer__close {
  position: absolute;
  top: 0;
  right: 0;
  z-index: 1;
  width: 72rpx;
  height: 72rpx;
  vertical-align: middle;
}

